{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@assets/*": ["./src/assets/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
